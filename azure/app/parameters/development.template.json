{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "deploymentBaseCommit": {
      "value": "${deploymentBaseCommit}"
    },
    "alertActionGroupShortName": {
      "value": "d01appalerts"
    },
    "appServiceDockerCompose": {
      "value": "${dockerCompose}"
    },
    "databaseName": {
      "value": "development"
    },
    "appServiceAlwaysOn": {
      "value": false
    },
    "databaseUsername": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/${subscriptionId}/resourceGroups/${keyVaultResourceGroupName}/providers/Microsoft.KeyVault/vaults/${keyVaultName}"
        },
        "secretName": "DatabaseUsername"
      }
    },
    "databasePassword": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/${subscriptionId}/resourceGroups/${keyVaultResourceGroupName}/providers/Microsoft.KeyVault/vaults/${keyVaultName}"
        },
        "secretName": "DatabasePassword"
      }
    },
    "alertEmailAddress": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/${subscriptionId}/resourceGroups/${keyVaultResourceGroupName}/providers/Microsoft.KeyVault/vaults/${keyVaultName}"
        },
        "secretName": "AlertEmailAddress"
      }
    },
    "SECRET_KEY_BASE": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/${subscriptionId}/resourceGroups/${keyVaultResourceGroupName}/providers/Microsoft.KeyVault/vaults/${keyVaultName}"
        },
        "secretName": "SecretKeyBase"
      }
    },
    "BASIC_AUTH_USERNAME": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/${subscriptionId}/resourceGroups/${keyVaultResourceGroupName}/providers/Microsoft.KeyVault/vaults/${keyVaultName}"
        },
        "secretName": "BasicAuthUsername"
      }
    },
    "BASIC_AUTH_PASSWORD": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/${subscriptionId}/resourceGroups/${keyVaultResourceGroupName}/providers/Microsoft.KeyVault/vaults/${keyVaultName}"
        },
        "secretName": "BasicAuthPassword"
      }
    },
    "DFE_SIGN_IN_ISSUER": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/${subscriptionId}/resourceGroups/${keyVaultResourceGroupName}/providers/Microsoft.KeyVault/vaults/${keyVaultName}"
        },
        "secretName": "DfeSignInIssuer"
      }
    },
    "DFE_SIGN_IN_IDENTIFIER": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/${subscriptionId}/resourceGroups/${keyVaultResourceGroupName}/providers/Microsoft.KeyVault/vaults/${keyVaultName}"
        },
        "secretName": "DfeSignInIdentifier"
      }
    },
    "DFE_SIGN_IN_SECRET": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/${subscriptionId}/resourceGroups/${keyVaultResourceGroupName}/providers/Microsoft.KeyVault/vaults/${keyVaultName}"
        },
        "secretName": "DfeSignInSecret"
      }
    },
    "DFE_SIGN_IN_REDIRECT_BASE_URL": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/${subscriptionId}/resourceGroups/${keyVaultResourceGroupName}/providers/Microsoft.KeyVault/vaults/${keyVaultName}"
        },
        "secretName": "DfeSignInRedirectBaseUrl"
      }
    },
    "NOTIFY_API_KEY": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/${subscriptionId}/resourceGroups/${keyVaultResourceGroupName}/providers/Microsoft.KeyVault/vaults/${keyVaultName}"
        },
        "secretName": "NotifyApiKey"
      }
    },
    "NOTIFY_TEMPLATE_ID": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/${subscriptionId}/resourceGroups/${keyVaultResourceGroupName}/providers/Microsoft.KeyVault/vaults/${keyVaultName}"
        },
        "secretName": "NotifyTemplateID"
      }
    },
    "ROLLBAR_ACCESS_TOKEN": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/${subscriptionId}/resourceGroups/${keyVaultResourceGroupName}/providers/Microsoft.KeyVault/vaults/${keyVaultName}"
        },
        "secretName": "RollbarAccessToken"
      }
    },
    "ROLLBAR_ENV": {
      "value": "development"
    }
  }
}
